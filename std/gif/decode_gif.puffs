pub error "bad GIF header"

pub struct decoder?(
	lzw lzw_decoder,
)

pub func decoder.decode?(dst ptr buf1, src ptr buf1)() {
	// Read either "GIF87a" or "GIF89a".
	if (in.src.read_u8?() != 0x47) or (in.src.read_u8?() != 0x49) or
		(in.src.read_u8?() != 0x46) or (in.src.read_u8?() != 0x38) {
		return error "bad GIF header"
	}
}
